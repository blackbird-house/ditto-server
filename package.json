{
  "name": "ditto-server",
  "version": "1.0.0",
  "description": "A ready-to-use easy-to-clone REST API",
  "main": "dist/server.js",
  "scripts": {
    "start": "NODE_ENV=production node dist/server.js",
    "dev": "NODE_ENV=development nodemon --exec ts-node src/server.ts",
    "staging": "NODE_ENV=staging node dist/server.js",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "NODE_ENV=test jest",
    "test:dev": "NODE_ENV=development jest",
    "test:staging": "NODE_ENV=staging jest",
    "test:prod": "NODE_ENV=production jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "type-check": "tsc --noEmit"
  },
  "packageManager": "yarn@1.22.19",
  "keywords": [
    "rest",
    "api",
    "server",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/sqlite3": "^5.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "sqlite3": "^5.1.7",
    "swagger-ui-express": "^5.0.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/yamljs": "^0.2.34",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
